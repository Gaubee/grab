{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "ESNext"
    ],
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "bundler",
    "types": [
      "node"
    ],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "verbatimModuleSyntax": true,
    "skipLibCheck": true,
  }
}